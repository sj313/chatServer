<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <!--To inherit the global NuGet package sources remove the <clear/> line below -->
    <clear />
    <add key="nuget" value="https://api.nuget.org/v3/index.json" />
  </packageSources>
  <config>
    <add key="repositoryPath" value="./resources/packages" />
    <add key="globalPackagesFolder" value="./resources/packages" />
  </config>
  <settings>
    <repositoryPath>./resources/packages</repositoryPath>
  </settings>
</configuration>
